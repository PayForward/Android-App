<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.payforward.androidapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SetLocationActivity"
            android:label="@string/title_activity_set_location" />
        <activity android:name=".SignUpTypeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpNameActivity" />
        <activity
            android:name=".previewscreens"
            android:label="@string/title_activity_previewscreens"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Description"></activity>
    </application>

</manifest>